    "@openzeppelin/contracts": "^4.4.2",
    "@truffle/hdwallet-provider": "^2.0.1",
    "@types/mocha": "^9.1.0",
    "chai": "^4.3.6",
    "dotenv": "^14.3.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "ethers": "^5.5.3",
