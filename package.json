    "migrate-kovan": "truffle migrate --network kovan --f 2 -to 2 --compile-all --reset",
    "verify-kovan": "truffle run verify DDAOallocV01 --network kovan",
    "migrate-ropsten": "truffle migrate --network ropsten --f 2 -to 2 --compile-all --reset",
    "verify-ropsten": "truffle run verify DDAOallocV01 --network ropsten",
    "test": "npx hardhat test",
    "test-cover": "npx hardhat coverage --solcoverjs ./.solcover.js",
